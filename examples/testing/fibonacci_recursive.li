#[unsafe]
#[vararg]
#[extern("C")]
fn printf(fmt: &[u8]) -> i32;

fn fibonacci(i: u32) -> u32{
    if i <= 2 {
        return 1
    } else {
        return fibonacci(i - 1) + fibonacci(i - 2)
    }
}

fn run_fib(i: u32){
    #[unsafe]
    printf(&"fib(%d) = %d", i, fibonacci(i));
}

fn main(){
    run_fib(1);
    run_fib(2);
    run_fib(3);
    run_fib(4);
    run_fib(5);
    run_fib(6);
    run_fib(7);
    run_fib(8);
    run_fib(9);
    run_fib(10);
    run_fib(11);
    run_fib(12);
}