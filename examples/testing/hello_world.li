#[unsafe]
#[extern("C")]
fn puts(msg: &) -> i32;

#[unsafe]
#[vararg]
#[extern("C")]
fn printf(fmt: &[u8]) -> i32;

fn char_at(arr: &[u8], index: uptr) -> u8 {
    let ptr = #[unsafe]{arr as uptr};
    let char = ptr as &u8;
    *char
}

fn main(){
    let c = char_at(&"hello", 1);
    #[unsafe]
    printf(&"%d\n", c);
}